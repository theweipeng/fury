package any_example;

message AnyInner [id=300] {
    string name = 1;
}

union AnyUnion [id=301] {
    string text = 1;
    bool flag = 2;
    AnyInner inner = 3;
}

message AnyHolder [id=302] {
    any bool_value = 1;
    any string_value = 2;
    any date_value = 3;
    any timestamp_value = 4;
    any message_value = 5;
    any union_value = 6;
    any list_value = 7;
    any map_value = 8;
}
